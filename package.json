{
  "name": "react-challenge-colorizr",
  "version": "1.0.0",
  "description": "Boilerplate for second React Challenge",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint:withoutfix && npm run testonly",
    "testonly": "cross-env NODE_ENV=development tape --require babel-register --require ignore-styles src/**/*.test.{js,jsx} | tap-notify | tap-spec",
    "start": "cross-env NODE_ENV=development babel-node server.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.babel.js --progress",
    "clean": "rimraf public",
    "lint": "eslint src --ext .js --ext .jsx --fix",
    "lint:withoutfix": "eslint src --ext .js --ext .jsx",
    "gh:build": "npm run clean && cross-env NODE_ENV=production BUILD=pages webpack --config webpack.config.babel.js --progress",
    "gh:deploy": "npm run gh:build && gh-pages -d public",
    "deploy": "npm run gh:deploy"
  },
  "pre-commit": [
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rtivital/react-redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "author": "Vitaly Rtishchev <rtivital@gmail.com> (http://github.com/rtivital)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rtivital/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/rtivital/react-redux-boilerplate#readme",
  "dependencies": {
    "axios": "^0.14.0",
    "breakpoint-sass": "^2.7.0",
    "classnames": "^2.2.5",
    "lodash": "^4.16.0",
    "node-uuid": "^1.4.7",
    "normalize-scss": "^5.0.3",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-fastclick": "^2.1.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0-beta.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-actions": "^0.12.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.2.9",
    "babel-plugin-module-resolver": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-imports": "^1.1.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-pure-class-to-function": "^1.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "colors": "^1.1.2",
    "cross-env": "^2.0.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.1",
